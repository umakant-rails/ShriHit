<div class="row" data-controller="">
  <div class="col-md-12">
    <div class="card">
      <div class="card-body h-400">
        <div class="page-subject-header">
          <div class="row">
            <div class="col-md-6">टैग्स सूची</div>
            <div class="col-md-6">
              <div class="btn-group fRight">
                <a href="javascript:void(0);" class="text-white btn btn-primary">
                  <i class="far fa-check-circle"></i> टैग्स सूची
                </a>
                <a href="<%= approved_tags_admin_tags_path %>" class="text-white btn btn-success">
                  <i class="far fa-check-circle"></i> स्वीकृत
                </a>
                <a href="<%= rejected_tags_admin_tags_path %>" class="text-white btn btn-danger">
                  <i class="fa-regular fa-circle-xmark"></i> अस्वीकृत
                </a>
              </div>
            </div>
          </div>
        </div>
        <div class="dropdown-divider"></div>
        <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>" data-admin-context-target="csrfToken">
        <div id="tag_list">
          <%= render "tags", tags: @tags, parent_type: params[:action] %>
        </div>
      </div>
    </div>
  </div>
</div>
