<!doctype html>
<html>
  <head>
    <meta charset='utf-8' />
    <%= wicked_pdf_stylesheet_link_tag "pdf" -%>
    <%= wicked_pdf_javascript_include_tag "number_pages" %>
  </head>
  <body onload='number_pages'>
    <div>
      <center>
        <%= image_tag wicked_pdf_asset_base64('shrihit-mini.png') %>
      </center>
    </div>
    <div id="content" style="background: wicked_pdf_asset_base64('shrihit.png');">
      <%= yield %>
    </div>
  </body>
</html>
