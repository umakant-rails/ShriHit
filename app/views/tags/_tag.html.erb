<div id="<%= dom_id tag %>">
</div>
<tr>
  <th scope="row">
    <%= local_assigns[:index] %>
  </th>
  <td>
    <%= link_to "" do %>
      <%= tag.name %>
    <% end %>
  </td>
  <td>
    <%= tag.is_approved ? 'स्वीकृत' : 'लंबित' %>
  </td>
  <td>
    <div class="row">
      <!-- <div class="col-md-2">
        <%#= link_to article_type do %>
          <i class="fa fa-eye"></i>
        <%# end %>
      </div> -->
      <div class="col-md-2">
        <%= link_to edit_tag_path(tag) do %>
          <i class="fa fa-edit"></i>
        <% end %>
      </div>
      <div class="col-md-2">
        <%= form_for(tag, html: { method: :delete, onSubmit: "return confirm('Are you sure you wish to delete?');" }) do |f| %>
          <button type="submit" class="btn" style="padding: 0;margin-bottom: 1px;">
            <i class="fa fa-trash" style="color:red;"></i>
          </button>
        <% end %>
      </div>
    </div>
  </td>
</tr>